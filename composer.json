{
  "name": "brzuchal/stop-the-war",
  "description": "PHP_CodeSniffer rule adding non-collapsable comment calling out to stop wars",
  "license": "MIT",
  "type": "phpcodesniffer-standard",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.4.0"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2 || ^0.7",
    "squizlabs/php_codesniffer": "^3.5.6"
  },
  "require-dev": {
    "phing/phing": "2.16.3",
    "phpunit/phpunit": "8.5.5|9.4.0",
    "slevomat/coding-standard": "^7.0",
    "doctrine/coding-standard": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "StopTheWar\\Sniffs\\": "StopTheWar/Sniffs/"
    }
  }
}
